#!/usr/bin/env node
var debug = require('debug')('Jupiter-server');
var app = require('../app');
var http = require('http').Server(app);
var autoPack = require('../../AutoPack/app.js');

app.set('port', process.env.PORT || 80);

http.listen(app.get('port'), function() {
  debug('Jupiter server listening on port ' + app.get('port'));
});
autoPack.start(http);